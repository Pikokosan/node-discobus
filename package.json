{
  "name": "discobus",
  "version": "1.0.1",
  "description": "The Disco Bus protocol, is a versatile master/slave protocol well suited for multidrop networks, like RS485.",
  "author": "Jeremy Gillick",
  "license": "MIT",
  "main": "./dist/discobus",
  "keywords": [
    "communication",
    "network",
    "rs485",
    "multidrop"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "test": "node_modules/mocha/bin/mocha",
    "test_watch": "node_modules/mocha/bin/mocha --watch",
    "build": "babel src --out-dir dist -s",
    "build_watch": "babel src --out-dir dist -s --watch",
    "coverage": "istanbul cover --print both _mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgillick/node-discobus.git"
  },
  "bugs": {
    "url": "https://github.com/jgillick/node-discobus/issues"
  },
  "homepage": "https://github.com/jgillick/node-discobus#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "proxyquire": "^1.7.10",
    "rxjs": "^5.0.0-beta.12",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.2"
  },
  "dependencies": {
    "crc": "^3.4.0",
    "serialport": "^4.0.1"
  },
  "typescript": {
    "definition": "index.d.ts"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015"
    ],
    "sourceRoot": "src"
  }
}
